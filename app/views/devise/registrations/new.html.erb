<div class>
  <div class="col-md-6 col-md-offset-3 text-center">
      <% if params[:plan] == '2' %>
       <h1 >Pro account</h1>
       <p >Sign up for Pro Account at $10/month for Access to the contact info and community Clint.</p>
      <% else %>
        <h1>Basic account</h1>
        <p>Sign up for basic access to community for Free!</p>
      <% end %>
  </div>  
  <div class='col-md-6 col-md-offset-3'>
    <div class='well'>
  
      <h2>Sign up</h2>
      <% if params[:plan] == '2'  %>
        <%= render "pro_form" %>
      <% else %>
        <%= render "basic_form" %>
      <% end %>
      
      <%= render "devise/shared/links" %>
      
    </div>
  </div>
</div>